arvutaKeskmine<-function(arvud, na.rm=T){
if (na.rm) {
arvud=arvud[!is.na(arvud)]
}
summa=sum(arvud)
n=length(arvud)
tulemus=summa/n
#tagastame tulemuse
return(tulemus)
}
